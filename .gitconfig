[user]
  email = awilbur@zulily.com
  name  = Alison Wilbur
[push]
  default = matching
[color]
  branch = auto
  diff   = auto
  status = auto
[core]
	editor = emacs
	autocrlf = false
[achievement]
  upload = false
[alias]
  s = status
  br = branch
  co = checkout
  d = diff
  dw = diff --word-diff
  a = add
  l = log
  lg1 = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
  lg2 = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
  lg = !"git lg1"
  lgd = log -p
  logd = log -p
  lgf = log --stat
  logf = log --stat
  prb = pull --rebase
[merge]
	tool = tortoisemerge
[diff]
	tool = kompare
[diff "kdiff3"]
	path = /usr/libexec/git-core/mergetools/kdiff3
